<h1>Blog</h1>

<div class="blogContainer">
    
        {{#if articles}}
        <div class="collection">
            {{#each articles}}
                <a href="/blog/{{_id}}" class="collection-item question">{{title}}</a> 
            {{/each}}
        </div>
        {{else}}
        <h1>There is no articles</h1>
        {{/if}}
   
</div>   